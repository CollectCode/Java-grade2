import java.util.Scanner;

public class Note1    {
    public static void main(String args[])    {
        Scanner scn = new Scanner(System.in);
        
        int[] nun = new int [3];
        int logic = 2;
        int maximum = 0;
        int same = 1;
        int totalprice = 0;
        
        for (int i = 0; i < nun.length; i++)    {
            nun[i] = scn.nextInt();
            if (maximum < nun[i])    {
                 maximum = nun[i];
            } 
            
            if (i >= 1 && nun[i] == nun[i-1])    {
                same = nun[i];
                logic -= 1;
            }
        }
        
        if (logic == 0)  {
            totalprice = 10000 + same * 1000;
        } else if (logic == 1)  {
            totalprice = 1000 + same * 100;
        } else if (logic == 2)	{
            totalprice = maximum * 100 ;
        }
        
        System.out.println(totalprice);
    }
}