import java.util.Scanner;

public class Note1   {
    public static void main(String args[])    {
        Scanner scn = new Scanner(System.in);
        
        int dan = scn.nextInt();
        
        for (int i = 0; i < 9; i++)    {
            System.out.println(dan + " * " + i + " = " + dan * (i+1));
        }
    }
}